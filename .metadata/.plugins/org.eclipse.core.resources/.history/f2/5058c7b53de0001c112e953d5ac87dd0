
public class infos {
	private int alunos[]= new int[5];
	private int professor[]= new int[5];
	private String escola[]= new String[5];
	public int numBus;
	public infos(int[] alunos, int[] professor, String[] escola, int numBus) {
		super();
		this.alunos = alunos;
		this.professor = professor;
		this.escola = escola;
		this.numBus = numBus;
	}
	public int[] getAlunos() {
		return alunos;
	}
	public void setAlunos(int[] alunos) {
		this.alunos = alunos;
	}
	public int[] getProfessor() {
		return professor;
	}
	public void setProfessor(int[] professor) {
		this.professor = professor;
	}
	public String[] getEscola() {
		return escola;
	}
	public void setEscola(String[] escola) {
		this.escola = escola;
	}
	
	
		

}
