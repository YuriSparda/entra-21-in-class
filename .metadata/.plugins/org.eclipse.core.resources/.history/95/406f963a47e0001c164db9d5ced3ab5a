import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		infos Prod[]=new infos[5];
		Prod[0]=new infos(0,0,"a",0);
		Prod[1]=new infos(0,0,"b",0);
		Prod[2]=new infos(0,0,"c",0);
		Prod[3]=new infos(0,0,"d",0);
		Prod[4]=new infos(0,0,"a",0);
		for (int i = 0; i < Prod.length; i++) {
		System.out.print("escreva o nome do produto : ");
		String nome =  sc.next();
		Prod[i].setName(nome);

		
		
		
		}
		System.err.println("FIN.");}

}
